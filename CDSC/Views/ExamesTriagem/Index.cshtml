@model CDSC.Models.DadosRecemNascidoModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("Salvar", "ExamesTriagem", FormMethod.Post))
{

    <div class="main">
        <!-- MAIN CONTENT -->
        <div class="main-content">
            <div class="container-fluid">
                <!-- OVERVIEW -->
                <div class="panel panel-headline">
                    <div class="panel-heading" @*style="text-align:center"*@>
                        <h2 class="panel-title">Dados do recém-nascido</h2>
                    </div>




                    <div class="panel-body">
                        <div class="panel-heading">
                            <h2 class="panel-title">Nascimento</h2>
                        </div>


                        <div class="row">
                            <div class="col-md-3">
                                <h3 class="panel-title">Nascido às</h3>
                                @Html.TextBoxFor(model => model.horaNascimento, new { @validate = "required", @class = "form-control", @maxlength = "5" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">Do dia</h3>
                                @Html.TextBoxFor(model => model.dataNascimento, new { @class = "form-control datepicker-here", @maxlength = "10" })
                            </div>
                            <div class="col-md-6">
                                <h3 class="panel-title">Maternidade</h3>
                                @Html.TextBoxFor(model => model.maternidade, new { @validate = "required", @class = "form-control", @maxlength = "50" })
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <br />


                        <div class="row">
                            <div class="col-md-3">
                                <h3 class="panel-title">UF</h3>
                                @Html.DropDownListFor(model => model.idUf, (SelectList)ViewBag.UF, "Selecione...", new { @class = "form-control ddluf" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">Município</h3>
                                @Html.DropDownListFor(model => model.idMunicipio, (SelectList)ViewBag.Municipio, "Selecione...", new { @class = "form-control ddlmunicipio" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">Peso ao Nascer(g)</h3>
                                @Html.TextBoxFor(model => model.peso, new { @validate = "required", @class = "form-control", @maxlength = "10" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">Comprimento ao Nascer(cm)</h3>
                                @Html.TextBoxFor(model => model.comprimento, new { @validate = "required", @class = "form-control", @maxlength = "10" })
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <br />



                        <div class="row">
                            <div class="col-md-3">
                                <h3 class="panel-title">Perímetro cefálico(cm)</h3>
                                @Html.TextBoxFor(model => model.perimetroEncefalico, new { @validate = "required", @class = "form-control", @maxlength = "10" })
                            </div>
                            <div class="col-md-3 form-inline">
                                <div class="form-group">
                                    <h3 class="panel-title">Sexo:</h3>
                                </div>
                                <div class="form-group">
                                    <label class="fancy-radio">
                                        @Html.RadioButtonFor(m => Model.sexo, 1, new { @class = "control-radio", @name = "radioSexoCrianca", @required = "required" })<span><i></i>Masculino</span>
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label class="fancy-radio">
                                        @Html.RadioButtonFor(m => Model.sexo, 2, new { @class = "control-radio", @name = "radioSexoCrianca" })<span><i></i>Feminino</span>
                                    </label>
                                </div>
                            </div>
