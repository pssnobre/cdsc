                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <br />


                        <div class="row">
                            <div class="col-md-3">
                                <h3 class="panel-title">Tipagem Sanguínea do RN:</h3>
                                @Html.TextBoxFor(model => model.tipoSanguineo, new { @validate = "required", @class = "form-control", @maxlength = "10" })
                            </div>
