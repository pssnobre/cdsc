                                <h3 class="panel-title">Apagar 1º min:</h3>
                                @Html.TextBoxFor(model => model.apagar1Minuto, new { @validate = "required", @class = "form-control", @maxlength = "10" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">Apagar 5º min:</h3>
                                @Html.TextBoxFor(model => model.apagar5minuto, new { @validate = "required", @class = "form-control", @maxlength = "10" })
                            </div>
                        </div>
