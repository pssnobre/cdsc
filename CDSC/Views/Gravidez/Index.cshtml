@model CDSC.Models.GravidezModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("Salvar", "Gravidez", FormMethod.Post))
{

    <div class="main">
        <!-- MAIN CONTENT -->
        <div class="main-content">
            <div class="container-fluid">
                <!-- OVERVIEW -->
                <div class="panel panel-headline">
                    <div class="panel-heading" @*style="text-align:center"*@>
                        <h2 class="panel-title">Dados sobre gravidez, parto e puerpério</h2>
                    </div>
                    
                    <div class="panel-body">
                        <div class="panel-heading">
                            <h2 class="panel-title">Pré-Natal</h2>
                        </div>


                        <div class="row">
                            <div class="col-md-6 form-inline">
                                <h3 class="panel-title">Pré natal inicioou no:</h3>
                                <div class="form-group">
                                    <label class="fancy-radio">
                                        @Html.RadioButtonFor(m => Model.inicioPreNatal, 1, new { @class = "control-radio", @name = "radioPeriodoPrenatal", @required = "required" })<span><i></i>1º Trimestre</span>
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label class="fancy-radio">
                                        @Html.RadioButtonFor(m => Model.inicioPreNatal, 2, new { @class = "control-radio", @name = "radioPeriodoPrenatal" })<span><i></i>2º Trimestre</span>
                                    </label>
                                </div>
                                <div class="form-group">
                                    <label class="fancy-radio">
                                        @Html.RadioButtonFor(m => Model.inicioPreNatal, 2, new { @class = "control-radio", @name = "radioPeriodoPrenatal" })<span><i></i>3º Trimestre</span>
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <h3 class="panel-title">Número de consultas pré natais</h3>
                                @Html.TextBoxFor(model => model.numeroConsultas, new { @class = "form-control", @maxlength = "10" })
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <br />



                        <div class="row">
                            <h3 class="panel-title">Sorologias realizadas no pré-natal para os agravos (Código do CID-10):</h3>
                            <div class="col-md-3">
                                <h3 class="panel-title">Z21: Status</h3>
                                @Html.DropDownListFor(model => model.statusZ21Prenatal, (SelectList)ViewBag.StatusSorologias, "Selecione...", new { @class = "form-control" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">Z21: Período</h3>
                                @Html.DropDownListFor(model => model.periodoZ21Prenatal, (SelectList)ViewBag.PeriodoSorologias, "Selecione...", new { @class = "form-control" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">A53: Status</h3>
                                @Html.DropDownListFor(model => model.statusA53Prenatal, (SelectList)ViewBag.StatusSorologias, "Selecione...", new { @class = "form-control" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">A53: Período</h3>
                                @Html.DropDownListFor(model => model.periodoA53Prenatal, (SelectList)ViewBag.PeriodoSorologias, "Selecione...", new { @class = "form-control" })
                            </div>

                        </div>
                        <div class="clearfix"></div>
                        <br />



                        <div class="row">
                            <div class="col-md-3">
                                <h3 class="panel-title">B18: Status</h3>
                                @Html.DropDownListFor(model => model.statusB18, (SelectList)ViewBag.StatusSorologias, "Selecione...", new { @class = "form-control" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">B18: Período</h3>
                                @Html.DropDownListFor(model => model.periodoB18, (SelectList)ViewBag.PeriodoSorologias, "Selecione...", new { @class = "form-control" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">B58: Status</h3>
                                @Html.DropDownListFor(model => model.statusB58, (SelectList)ViewBag.StatusSorologias, "Selecione...", new { @class = "form-control" })
                            </div>
                            <div class="col-md-3">
                                <h3 class="panel-title">B58: Período</h3>
                                @Html.DropDownListFor(model => model.periodoB58, (SelectList)ViewBag.PeriodoSorologias, "Selecione...", new { @class = "form-control" })
