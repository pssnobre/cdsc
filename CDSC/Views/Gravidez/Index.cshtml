                                <div class="form-group">
                                    <label class="fancy-radio">
                                        @Html.RadioButtonFor(m => Model.megadoseVitaminaA, 3, new { @class = "control-radio", @name = "radioTipoParto" })<span><i></i>Sem Informação</span>
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <br />


                        <div class="row">
                            <div class="col-md-12">
                                <h3 class="panel-title">Intercorrências clínicas da gravidez. parto e puerpério:</h3>
                                @Html.TextAreaFor(model => model.intercorrenciasClinicas, new { @validate = "required", @rows = "3", @class = "form-control", @maxlength = "200" })
                            </div>
                        </div>

                        <div class="clearfix"></div>
                        <br />






